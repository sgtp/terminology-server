##########################################
# TableC-1 
##########################################	
map:tableC1 a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "common/c-1";							# TODO a constant value should be used instead
	d2rq:class metaMap:TerminologySet;						# TODO going to be obsoleted
	.
map:tableC1Label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property rdfs:label;
	d2rq:constantValue "Table C-1"@en;
	.
map:tableC1Comment a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property rdfs:comment;
	d2rq:constantValue "Originating center"@en;				# TODO to verify
	.	
map:tableC1NS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property metaMap:nameSpace;
	d2rq:constantValue "c-1";
	.	
map:tableC1In a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property metaMap:containedIn;
	d2rq:constantValue <http://reference.metoffice.gov.uk/data/wmo/tdcf/common>;
	.
map:tableC1Version a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property metExtra:hasVersion;
	d2rq:constantValue "i0";
	.	
#map:tableC1Type a d2rq:PropertyBridge;
#	d2rq:belongsToClassMap map:tableC1;
#	d2rq:property rdf:type;
#	d2rq:constantValue iso19135:Registry;
#	.	
map:tableC1Pragma a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tableC1;
	d2rq:property metaMap:pragma;
	d2rq:constantValue generic:canonicalExpandDashSuppressC1;
	.




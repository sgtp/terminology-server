##########################################
# Mapping elements in Table C1
##########################################											
map:C1 a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "common/c-1/@@C-1.CodeFigureForF3F3F3@@";
	d2rq:class metaMap:TerminologyIndividual;
	d2rq:condition "C-1.CodeFigureForF3F3F3!=\"\"";
	.
map:C1SkosNotation a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property skos:notation;
	d2rq:column "C-1.CodeFigureForF3F3F3";
	.

map:C1SkosLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property skos:prefLabel;
	d2rq:column "C-1.OriginatingGeneratingCentres_en";
	d2rq:condition "C-1.Reserved_by=\"\"";
	d2rq:lang "en";
	.
map:C1SkosLabel2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property rdfs:label;
	d2rq:constantValue "Reserved"@en;
	d2rq:condition "C-1.Reserved_by!=\"\"";
	.	
map:C1Comment1 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property rdfs:comment;
	d2rq:pattern "Reserved by @@C-1.Reserved_by@@";
	d2rq:condition "C-1.Reserved_by!=\"\"";
	d2rq:lang "en";
	.	

#map:C1TypeRegistryItem a d2rq:PropertyBridge;
#	d2rq:belongsToClassMap map:C1;
#	d2rq:property rdf:type;
#	d2rq:constantValue iso19135:RegisterItem;
#	.
map:C1TypeSKOSConcept a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property rdf:type;
	d2rq:constantValue skos:Concept;
	d2rq:condition "C-1.Reserved_by=\"\"";
	.		

#map:C1TypeSKOSInScheme a d2rq:PropertyBridge;
#	d2rq:belongsToClassMap map:C1;
#	d2rq:property skos:inScheme;
#	d2rq:refersToClassMap map:dataUnitScheme;
#	.	
map:C1TypeInRegistry a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property metaMap:containedIn;
	d2rq:refersToClassMap map:tableC1;
	.
map:C1TypeNS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property metaMap:nameSpace;
	d2rq:column "C-1.CodeFigureForF3F3F3";
	.

map:C1TypeStatus a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:status;
	d2rq:uriColumn "C-1.Status";
	d2rq:translateWith map:StatusTableWMO;
	.
#map:C1TypeStatus2 a d2rq:PropertyBridge;
#	d2rq:belongsToClassMap map:C1;
#	d2rq:property iso19135:status;
#	d2rq:uriColumn "C-1.Status";
#	d2rq:translateWith map:StatusTableISO;
#	.

map:C1Reserved a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:status;
	d2rq:constantValue wmo:Reserved;
	d2rq:condition "C-1.OriginatingGeneratingCentres_en like \"Reserved%\" or Reserved_by!=\"\"";
	.
map:C1Match a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property skos:exactMatch;
	d2rq:refersToClassMap map:C11;
	d2rq:condition "C-1.Octet5GRIB1_Octet6BUFR3 = C-11.GRIB2_BUFR4 and C-1.Octet5GRIB1_Octet6BUFR3 not like \"%-%\"";
	d2rq:condition "C-1.Reserved_by=\"\"";
	.	
map:C1Match2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property rdfs:seeAlso;
	d2rq:refersToClassMap map:C11;
	d2rq:condition "C-1.Octet5GRIB1_Octet6BUFR3 = C-11.GRIB2_BUFR4 and C-1.Octet5GRIB1_Octet6BUFR3 not like \"%-%\"";
	d2rq:condition "C-1.Reserved_by!=\"\"";
	.		

map:C1InRegion a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:inRegion;
	d2rq:uriColumn "C-1.Region";
	d2rq:translateWith map:regions;
	.	

map:C1Version a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property metExtra:hasVersion;
	d2rq:constantValue "i0";
	.	
# Here are local fields

map:C1CodeFigureForF1F2 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:CodeFigureForF1F2;
	d2rq:column "C-1.CodeFigureForF1F2";
	.

map:C1CodeFigureForF3F3F3 a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:CodeFigureForF3F3F3;
	d2rq:column "C-1.CodeFigureForF3F3F3";
	.

map:C1Octet5GRIB1_Octet6BUFR a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:Octet5GRIB1_Octet6BUFR3;
	d2rq:column "C-1.Octet5GRIB1_Octet6BUFR3";
	.
	
map:C1OriginatingGeneratingCentres_en a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C1;
	d2rq:property wmo:OriginatingGeneratingCentres_en;
	d2rq:column "C-1.OriginatingGeneratingCentres_en";
	.			



##########################################
# Schema: unit of measure
##########################################	

map:dataUnitScheme a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "schemes/dataUnits";
	d2rq:class metaMap:TerminologySet;
	.
map:dataUnitSchemeNS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property metaMap:nameSpace;
	d2rq:constantValue "dataUnits";
	.		
map:dataUnitSchemeContained a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property metaMap:containedIn;
	d2rq:constantValue <http://reference.metoffice.gov.uk/data/wmo/tdcf/common>;
	.	
map:dataUnitSchemeDescription a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property rdfs:comment;
	d2rq:constantValue "Data units in use in GRIB"@en;
	.
map:dataUnitSchemeLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property rdfs:label;
	d2rq:constantValue "Data units"@en;
	.	
map:dataUnitSchemeSKOSType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property rdf:type;
	d2rq:constantValue skos:ConceptScheme;
	.	
map:dataUnitSchemeTop a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property skos:hasTopConcept;
	d2rq:refersToClassMap map:dataUnitTopConcept;
	.
map:dataUnitSchemeVersion a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitScheme;
	d2rq:property metExtra:hasVersion;
	d2rq:constantValue "i0";
	.
	
	
	
	
	
##########################################
# x-register for data units
##########################################		
	
map:x-dataUnits a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "common/x-dataUnits";
	d2rq:class metaMap:TerminologySet;
	.
map:x-dataUnitsRegistryNS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:x-dataUnits;
	d2rq:property metaMap:nameSpace;
	d2rq:constantValue "x-dataUnits";
	.		
map:x-dataUnitsRegistryContained a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:x-dataUnits;
	d2rq:property metaMap:containedIn;
	d2rq:constantValue <http://reference.metoffice.gov.uk/data/wmo/tdcf/common>;
	.	
map:x-dataUnitsRegistryDescription a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:x-dataUnits;
	d2rq:property rdfs:comment;
	d2rq:constantValue "A collection of data units, in addition to the one defined in BUFR/CREX/GRIB"@en;
	.
map:x-dataUnitsRegistryLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:x-dataUnits;
	d2rq:property rdfs:label;
	d2rq:constantValue "Data units"@en;
	.	
map:x-dataUnitsRegistryType a d2rq:PropertyBridge;

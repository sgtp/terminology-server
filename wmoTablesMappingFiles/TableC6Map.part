	d2rq:constantValue "i0";
	.	


##########################################
# data unit top concept
##########################################	
map:dataUnitTopConcept a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "common/x-dataUnits/dataUnit" ;
	d2rq:class metaMap:TerminologyIndividual;
	.
map:dataUnitTopConceptNS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property metaMap:nameSpace;
	d2rq:constantValue "dataUnit"@en;
	.	
map:dataUnitTopConceptType a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property rdf:type;
	d2rq:constantValue skos:Concept;
	.	
map:dataUnitTopConceptLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property rdfs:label;
	d2rq:constantValue "data unit"@en;
	.	
map:dataUnitTopConceptComment a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property rdfs:comment;
	d2rq:constantValue "Any data unit"@en;
	.	
map:dataUnitTopConceptInRegistry a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property metaMap:containedIn;
	d2rq:refersToClassMap	map:x-dataUnits;
	.
map:dataUnitTopConceptInScheme a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property skos:inScheme;
	d2rq:refersToClassMap	map:dataUnitScheme;
	.	
map:dataUnitTopConceptVersion a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:dataUnitTopConcept;
	d2rq:property metExtra:hasVersion;
	d2rq:constantValue "i0";
	.	



##########################################
# Mapping individuals in C-6 
# (unit of measures types)
# We planned to use this in the mapping, 
# but this is currently unused
##########################################

map:C6types a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "common/x-dataUnits/t@@C-6.typeOfUnitCode@@";
	d2rq:class metaMap:TerminologyIndividual;
	.
map:C6typesNotation a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property skos:notation;
	d2rq:column "C-6.typeOfUnitCode";
	.
map:C6typesLabel a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property skos:prefLabel;
	d2rq:column "C-6.TypeOfUnit"@en;
	.
map:C6typesSkosBroader a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property skos:broader;
	d2rq:refersToClassMap map:dataUnitTopConcept;
	.		
map:C6typesSKOSConcept a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property rdf:type;
	d2rq:constantValue skos:Concept;
	.		
map:C6typesSKOSInScheme a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property skos:inScheme;
	d2rq:refersToClassMap map:dataUnitScheme;
	.	
map:C6typesInRegistry a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property metaMap:containedIn;
	d2rq:refersToClassMap map:x-dataUnits;
	.
map:C6typesNS a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property metaMap:nameSpace;
	d2rq:pattern "t@@C-6.typeOfUnitCode@@";
	.

map:C6typesVersion a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:C6types;
	d2rq:property metExtra:hasVersion;
	d2rq:constantValue "i0";
	.



